@model IEnumerable<MyClass.Models.Menu>
@{ 
    var menu = ViewBag.Menu;
}
<li class="nav-item dropdown" style="font-weight: bold;">
    <a class="nav-link dropdown-toggle" href="@menu.Link" role="button" data-bs-toggle="dropdown" aria-expanded="false">
        @menu.Name
    </a>
    <ul class="dropdown-menu">
        @foreach (var item in Model)
        {
            <li><a class="dropdown-item" style="font-weight: bold;" href="@item.Link">@item.Name</a></li>
        }
    </ul>
</li>
